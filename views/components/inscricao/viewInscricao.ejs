<div class="container rounded shadow bg-white p-4 ">
    <div class="row">
      <div class="text-center">  <h2> Gestão Ativa </h2> </div>
        <div class="col-sm text-end"> 
            <p class="fs-5"> Gestão: <%= cipa.gestao + '/' + (cipa.gestao + 1) %> </p>
            <p class="fs-5"> Inscrições: <%= cipa.dtiniinsc %> à <%= cipa.dtfiminsc %></p>
        </div>
        <div class="col-sm"> 
            <p class="fs-5"> Votação: <%= cipa.dtinivoto %> à <%= cipa.dtfimvoto %> </p>
            <p class="fs-5"> Resultado: <%= cipa.dtresultado %> </p>
        </div>
        <div class="text-end">
            <form action="/solicitar_alteracao?_method=DELETE" method="POST">
                <a class="btn btn-success" href="/votacao"> Iniciar Votação </a>
                <a class="btn btn-color" href="/cadastro_candidato"> Cadastrar Candidatos </a>
                <button name="deletedcipaid" class="btn btn-danger" type="submit" value="<%= cipa.id %>"> Solicitar Alteração </button>
            </form>
        </div>
    </div>
</div>