        <div class="container ">
            <div class="row">
                <div class="col-sm text-center align-text-bottom pt-5">
                    <img src='cipa.png' class="w-50"/>
                </div>
                <div class="col-sm rounded shadow bg-white p-3">
                    <form class="formgroup" action="/cipaconfig" method="POST">
                        <div id="inscription" class="row justify-content-center">
                            <div class="text-center">  <h2> Gestão <%= gestao  %> </h2> </div>
                            <div  class="text-center mb-2">
                                <h3 class="title-style pt-2"> Inscrições </h3> 
                                <div class="row pt-4">
                                    <div class="col px-2">
                                        <div id="datainscricao" class="input-daterange justify-content-center input-group">
                                            <input class="input-sm form-control" type="text" id="StartInscription"  onkeydown="return false;" name="inscricaoini" required>
                                            <span class="p-2">até</span>
                                            <input class="input-sm form-control" type="text" id="EndInscription"  onkeydown="return false;" name="inscricaofim" required>
                                        </div>
                                    </div>
                                </div>
                            </div>
                                <div class="text-center mb-2">
                                    <h3 class="title-style pt-2" > Votação </h3> 
                                    <div class="row pt-4">
                                        <div class="col px-2">
                                            <div id="datavotacao" class="input-daterange justify-content-center input-group">
                                                <input class="input-sm form-control" type="text" id="StartVote"  onkeydown="return false;" name="votacaoini" required>
                                                <span class="p-2">até</span>
                                                <input class="input-sm form-control" type="text" id="EndVote"  onkeydown="return false;" name="votacaofim" required>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                        </div>
                        <div class="row">
                            <div class="col-sm text-center py-3">
                                <span> Resultado final:</span>
                                <input id="resultado-final" name="resultado" type="text" class="input-style" onkeydown="return false;" required>
                            </div>
                            </div>
                        <div class="row pt-2">
                            <div class="col text-center">
                                <button type="submit" class="btn btn-color"> Salvar Dados </button>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        
    </div>
       
    